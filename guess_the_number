# The below program checks if the guessed number by you
# is same as random number generated by the computer

import random

def guess(x):
    random_number=random.randint(1,x)
    guess=0
    while guess!=random_number:
        guess=int(input("Enter a number: "))
        if guess<random_number:
            print("The guessed number is too low")
        elif guess>random_number:
            print("The guessd number is too high")
    print(f"yay',you guessed {guess} and it is correct number ")


guess(6)